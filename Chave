	}

	public static int getBolas() {
		return bolas;
	}

	public static int getEstrelas() {
		return estrelas;
	}

	public int getBolas(int j) {
		// TODO Auto-generated method stub
		return 0;
	}

	public int getEstrelas(int j) {
		// TODO Auto-generated method stub
		return 0;
	}

}

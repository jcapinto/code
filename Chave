public class Chave {
	private int[] bolas;
    private int[] estrelas;
    private static final int NUMBOLAS = 5;
    private static final int NUMESTRELAS = 2;
    private int ano;
    private int semana;
    
    

    public Chave() {
        this.ano = 0;
        this.semana = 0;
        this.bolas = new int[5];
        this.estrelas = new int[2];
    }

    public Chave(int ano, int semana, int[] bolas, int[] estrelas) {
        this.ano = ano;
        this.semana = semana;
        this.bolas = bolas;
        this.estrelas = estrelas;
    }

    public Chave(int[] numeros, int[] estrelas) {
        this.bolas = numeros;
        this.estrelas = estrelas;
    }

    public void ordenarNumeros() {
        int n = bolas.length;
        for (int i = 0; i < n-1; i++) {
            for (int j = 0; j < n-i-1; j++) {
                if (bolas[j] > bolas[j+1]) {
                    int temp = bolas[j];
                    bolas[j] = bolas[j+1];
                    bolas[j+1] = temp;
                }
            }
        }
    }
    public int getAno() {
        return this.ano;
    }

    public void setAno(int ano) {
        this.ano = ano;
    }

    public int getSemana() {
        return this.semana;
    }

    public void setSemana(int semana) {
        this.semana = semana;
    }
    
    
    public int[] getBolas() {
        return bolas;
    }

    public void setBolas(int[] bolas) {
        this.bolas = bolas;
    }

    public int[] getEstrelas() {
        return estrelas;
    }

    public void setEstrelas(int[] estrelas) {
        this.estrelas = estrelas;
    }

    public static int getNumBolas() {
        return NUMBOLAS;
    }

    public static int getNumEstrelas() {
        return NUMESTRELAS;
    }

    }
